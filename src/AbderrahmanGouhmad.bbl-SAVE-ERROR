    \entry{baskara_personalised_2023}{article}{}
      \name{author}{1}{}{%
        {{un=0,uniquepart=base,hash=785f9a32dcf7b12e4ebe15134525b50d}{%
           family={Baskara},
           familyi={B\bibinitperiod},
           given={Risang},
