      \strng{namehash}{f1cd8389736a1ec5909fa953b46459bf}
      \strng{fullhash}{63d16e97851bab23266699b331e1aa97}
      \strng{bibnamehash}{63d16e97851bab23266699b331e1aa97}
      \strng{authorbibnamehash}{63d16e97851bab23266699b331e1aa97}
